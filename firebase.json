{
  "firestore": {
    "rules": "firestore.rules"
  },
  "hosting": {
    "public": "frontend-app/build",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "functions": {
    "source": "backend-services",
    "codebase": "default"
  }
}
